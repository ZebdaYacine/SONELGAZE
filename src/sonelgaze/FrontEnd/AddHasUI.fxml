<GridPane id="AnchorPane" prefHeight="270.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sonelgaze.UIControllers.AddHasUIController">
